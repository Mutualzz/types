{
    "bugs": {
        "url": "https://github.com/Mutualzz/types.git/issues"
    },
    "devDependencies": {
        "@types/bun": "latest"
    },
    "exports": {
        ".": {
            "import": "./dist/index.js",
            "types": "./dist/index.d.ts"
        }
    },
    "files": [
        "dist/**",
        "dist"
    ],
    "main": "./dist/index.js",
    "name": "@mutualzz/types",
    "peerDependencies": {
        "typescript": "^5.8.3"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/Mutualzz/types.git"
    },
    "scripts": {
        "build": "bun run check-types && bunchee",
        "check-types": "tsc --noEmit",
        "dev": "bunchee --watch",
        "prepublishOnly": "bun run build"
    },
    "type": "module",
    "types": "dist/index.d.ts",
    "version": "1.3.0"
}
